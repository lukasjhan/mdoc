  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "prettier": "catalog:"
  },
  "devDependencies": {
    "@protokoll/tsconfig": "workspace:*",
    "typescript": "catalog:"
  },
  "prettier": "@protokoll/prettier-config"
}
