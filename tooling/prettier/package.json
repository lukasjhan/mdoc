{
  "name": "@protokoll/prettier-config",
  "description": "protokoll Prettier Config",
  "author": "Martin Auer",
  "private": true,
  "version": "0.2.1",
  "type": "module",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
