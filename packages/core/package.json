{
  "name": "@protokoll/core",
  "description": "Protokoll Core Functionality",
  "author": "Martin Auer",
  "version": "0.2.35",
  "private": false,
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
