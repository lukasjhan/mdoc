    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "node --test"
  },
  "dependencies": {
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@ausweis/eslint": "catalog:",
    "@ausweis/prettier": "catalog:",
    "@ausweis/typescript": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "type-fest": "^4.26.1",
    "typescript": "catalog:"
  },
  "prettier": "@ausweis/prettier",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protokoll/packages/core"
  }
